{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "a1a/table-of-content",
	"version": "0.1.0",
	"title": "Table Of Content (A1A)",
	"category": "widgets",
	"icon": "smiley",
	"description": "Table of content.",
	"supports": {
		"html": false
	},
	"textdomain": "table-of-content",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"keywords": ["a1a", "content"],
	"attributes": {
		"containerClass": {
			"type": "string",
			"default": ".entry-content.wp-block-post-content" 
		}
	},
	"example": {
		"attributes": {
			"containerClass": ".entry-content.wp-block-post-content"
		}
	}
}